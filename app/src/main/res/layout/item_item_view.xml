<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@android:drawable/editbox_background_normal"
    android:descendantFocusability="blocksDescendants">

    <Button
        android:id="@+id/listItem_delete_button"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:background="@android:drawable/ic_menu_delete" />

    <TextView
        android:id="@+id/item_itemName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="@dimen/default_text_margin"
        android:layout_toLeftOf="@+id/listItem_delete_button"
        android:layout_toStartOf="@+id/listItem_delete_button"
        android:fontFamily="sans-serif"
        android:text="@string/error"
        android:textAlignment="center"
        android:textSize="@dimen/default_sub_text_size"
        android:textStyle="bold" />


    <TextView
        android:id="@+id/item_itemUpdateDate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/item_itemName"
        android:layout_marginTop="@dimen/default_text_margin"
        android:fontFamily="sans-serif"
        android:text="@string/error"
        android:textSize="@dimen/default_sub_text_size" />

    <LinearLayout
        android:id="@+id/linearLayout14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/item_itemUpdateDate"
        android:layout_marginTop="@dimen/default_text_margin"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif"
            android:text="@string/price"
            android:textSize="@dimen/default_sub_text_size" />

        <TextView
            android:id="@+id/textView31"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/colon"
            android:textSize="@dimen/default_sub_text_size" />

        <TextView
            android:id="@+id/item_itemPrice"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif"
            android:text="@string/error"
            android:textSize="@dimen/default_text_size"
            android:textStyle="italic" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif"
            android:text="@string/money_shortcut"
            android:textAllCaps="false"
            android:textSize="@dimen/default_sub_text_size" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/linearLayout14"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="sans-serif"
            android:text="@string/code"
            android:textSize="@dimen/default_sub_text_size" />

        <TextView
            android:id="@+id/textView32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/colon"
            android:textSize="@dimen/default_sub_text_size" />

        <TextView
            android:id="@+id/item_itemId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/error"
            android:textSize="@dimen/default_text_size"
            android:textStyle="italic" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/linearLayout13"
        android:layout_alignStart="@+id/linearLayout13"
        android:layout_below="@+id/linearLayout13"
        android:layout_marginTop="@dimen/default_text_margin"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="ZniÅ¼ki" />

        <TextView
            android:id="@+id/textView47"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/default_text_margin"
            android:layout_weight="1"
            android:text="@string/colon" />

        <TextView
            android:id="@+id/dis_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/default_text_margin"
            android:layout_weight="1"
            android:text="5%"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/dis_10"
            android:layout_width="wrap_content"
