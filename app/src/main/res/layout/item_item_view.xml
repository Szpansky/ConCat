            android:layout_marginEnd="@dimen/default_text_margin"
            android:layout_weight="1"
            android:text="15%"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/dis_20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/default_text_margin"
            android:layout_weight="1"
            android:text="20%"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/dis_25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/default_text_margin"
            android:layout_weight="1"
            android:text="25%"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/dis_30"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/default_text_margin"
            android:layout_weight="1"
            android:text="30%"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/dis_35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/default_text_margin"
            android:layout_weight="1"
            android:text="35%"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/dis_40"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/default_text_margin"
            android:layout_weight="1"
            android:text="40%"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/dis_100"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/default_text_margin"
            android:layout_weight="1"
            android:text="100%"
            android:textSize="12sp"
            android:textStyle="bold" />
    </LinearLayout>


</RelativeLayout>